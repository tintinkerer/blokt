{
  "manifest_version": 3,
  "name": "Blocklist Addon",
  "version": "1.0",
  "description": "Blocks URLs and maintains a blocklist",
  "permissions": ["storage", "tabs"],
  "background": {
  "persistent": "true",
    "scripts": "background.js"
  },
  "options": {
    "entries": ["blocklist"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  }
}
